    "version": "1.0.0",
    "description": "Static gitbook documentation site",
    "scripts": {
        "dev": "npx http-server src -p 8000",
        "deploy": "wrangler pages deploy src --project-name=my-blog",
        "docs:build": "honkit build",
        "docs:serve": "honkit serve"
    },
    "devDependencies": {
        "gitbook-plugin-codetabs": "^1.0.0",
        "gitbook-plugin-hints": "^1.0.2",
        "gitbook-plugin-tabs": "^0.0.7",
        "honkit": "^6.0.0"
    },
    "keywords": [
        "documentation",
        "gitbook",
        "static-site"
    ],
    "author": "",
